{
  "name"    : "smplphp/logic",
  "require"          : {
    "php": "^8.3"
  },
  "require-dev"      : {
    "phpunit/phpunit"          : "^10.5",
    "phpstan/phpstan"          : "^1.10",
    "vimeo/psalm"              : "^5.24",
    "infection/infection"      : "^0.27.11",
    "symplify/monorepo-builder": "^11.2"
  },
  "license"          : "MIT",
  "authors"          : [
    {
      "name" : "Ollie",
      "email": "code@ollie.codes"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Smpl\\Logic\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Smpl\\Logic\\Tests\\": "tests"
    }
  },
  "config"      : {
    "allow-plugins": {
      "infection/extension-installer": true
    }
  }
}
